{
    "name": "latex-ast-parser",
    "description": "JavaScript LaTeX parser and beautifier",
    "version": "0.6.2",
    "author": {
        "name": "Jason Siefken",
        "email": "siefkenj@gmail.com"
    },
    "keywords": [
        "pegjs",
        "latex",
        "parser",
        "prettier"
    ],
    "main": "dist/latex-parser.js",
    "types": "dist/src/parsers/latex-parser.d.ts",
    "files": [
        "dist/latex-parser.js",
        "dist/src"
    ],
    "scripts": {
        "debug": "node --nolazy --inspect-brk=9229 ./dist/latex-parser.js",
        "test": "jest src/tests",
        "watch": "webpack --watch",
        "build": "mkdirp dist; webpack"
    },
    "dependencies": {
        "@types/color": "^3.0.2",
        "@types/cssesc": "^3.0.0",
        "@types/prettier": "^2.4.2",
        "@types/trie-prefix-tree": "^1.5.0",
        "color": "^4.1.0",
        "cssesc": "^3.0.0",
        "prettier": "^2.5.1",
        "trie-prefix-tree": "^1.5.1"
    },
    "devDependencies": {
        "@babel/core": "7.16.5",
        "@babel/preset-env": "7.16.5",
        "@types/jest": "^27.0.3",
        "@types/node": "^17.0.4",
        "babel-jest": "^27.4.5",
        "babel-loader": "8.2.3",
        "eslint": "^8.5.0",
        "jest": "27.4.5",
        "mkdirp": "1.0.4",
        "pegjs": "0.10.0",
        "pegjs-loader": "^0.5.6",
        "prettier-plugin-pegjs": "^0.4.0",
        "ts-jest": "^27.1.2",
        "ts-loader": "^9.2.6",
        "tslib": "^2.3.1",
        "typescript": "^4.5.4",
        "webpack": "5.65.0",
        "webpack-cli": "^4.9.1"
    },
    "jest": {
        "transform": {
            "\\.js$": "babel-jest",
            "latex.pegjs$": "<rootDir>/src/tests/pegjs-preprocessor-latex.js",
            "\\.pegjs$": "<rootDir>/src/tests/pegjs-preprocessor.js",
            "^.+\\.tsx?$": "ts-jest"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/siefkenj/latex-parser.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/siefkenj/latex-parser/issues"
    },
    "homepage": "https://github.com/siefkenj/latex-parser#readme",
    "engines": {
        "node": ">= 10.0"
    }
}

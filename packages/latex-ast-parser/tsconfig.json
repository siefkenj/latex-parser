{
    "compilerOptions": {
        "outDir": "./dist",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "module": "es6",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": true,
        "sourceMap": true,
        "declaration": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    }
}
